{
  "env": {
    "browser": true,
    "amd": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "func-names": ["error", "never"],
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-multi-spaces": "error",
    "no-unused-vars": "error",
    "prefer-const": "error",
    "no-console": 1
    
  }
}
